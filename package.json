{
  "name": "study-electron",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts-ts": "3.1.0"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "electron": "electron .",
    "dev": "nf start -p 3000",
    "win-package": "electron-packager . study-electron --platform=win32 --arc=x64 --out=release",
    "mac-package": "electron-packager . study-electron --platform=darwin --arc=x64 --out=release"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "electron": "^4.0.1",
    "electron-packager": "^13.0.1",
    "foreman": "^3.0.1",
    "typescript": "^3.2.2"
  }
}
